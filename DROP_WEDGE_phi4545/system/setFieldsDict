FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    sphereToCell
    {
         centre (0.018 0.0153 0.018); 
           radius 0.0014 ;
        fieldValues
        (
            volScalarFieldValue alpha.water 1
            volVectorFieldValue U ( 0 -0.5 0)
        );
    }
      
                                                       
   
 
);


